    <wsdl:operation name="SendSMS">
      <soap:operation soapAction="SendSMS" style="rpc" />
      <wsdl:input>
        <soap:body use="encoded" namespace="www.smsmelli.com" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" />
      </wsdl:input>
      <wsdl:output>
        <soap:body use="encoded" namespace="www.smsmelli.com" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" />
